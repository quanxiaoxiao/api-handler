{
  "extends": ["eslint-config-airbnb"],
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "parser": "babel-eslint",
    "ecmaVersion": 2019,
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": 0,
    "no-bitwise": 0,
    "no-continue": 0,
    "import/extensions": 0,
    "no-mixed-operators": 0,
    "no-underscore-dangle": 0,
    "import/prefer-default-export": 0,
    "no-param-reassign": 0,
    "class-methods-use-this": 0,
    "no-plusplus": 0
  },
  "globals": {}
}
